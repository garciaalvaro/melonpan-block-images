{
	"name": "melonpan-block-images",
	"version": "1.0.0",
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/editor.config.js && webpack --mode development --watch --config ./.webpack/front.config.js",
		"prod": "webpack --mode production --config ./.webpack/editor.config.js & webpack --mode production --config ./.webpack/front.config.js",
		"copy": "webpack --config ./.webpack/copy.config.js",
		"replace": "webpack --config ./.webpack/replace.config.js",
		"remove": "rm _extras/releases/latest/_replace.js _extras/releases/latest/_copy.js _extras/releases/latest/build/melonpan-block-images-front.js",
		"release": "npm run prod && npm run copy && npm run replace && npm run remove",
		"type": "tsc"
	},
	"alias": {
		"utils": "./src/js/utils"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.4.4",
		"@types/lodash": "^4.14.133",
		"@types/node": "^12.0.4",
		"@types/react-dom": "^16.8.4",
		"@types/tinycolor2": "^1.4.2",
		"@types/uuid": "^3.4.4",
		"@wordpress/blocks": "^6.3.0",
		"@wordpress/components": "^7.4.0",
		"@wordpress/editor": "^9.3.0",
		"@wordpress/element": "^2.4.0",
		"@wordpress/i18n": "^3.4.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"lodash": "^4.17.11",
		"merge2": "^1.2.3",
		"mini-css-extract-plugin": "^0.7.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"string-replace-loader": "^2.1.1",
		"style-loader": "^0.23.1",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.5.1",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2"
	},
	"dependencies": {
		"tinycolor2": "^1.4.1"
	},
	"wp": {
		"plugin_name": "Melonpan Block - Images",
		"plugin_uri": "https://wordpress.org/plugins/melonpan-block-images/",
		"plugin_description": "Add a gallery of images in a simple but clean layout.",
		"plugin_author": "melonpan",
		"plugin_version": "1.0.0",
		"plugin_tested_up_to": "5.2",
		"plugin_requires_at_least": "5.2",
		"plugin_php_version": "5.6",
		"plugin_tags": "gutenberg, blocks, editor, images, slider, carousel"
	}
}
