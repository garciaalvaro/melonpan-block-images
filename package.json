{
	"name": "melonpan-block-images",
	"plugin_name": "Melonpan Block - Images",
	"plugin_uri": "https://wordpress.org/plugins/melonpan-block-images/",
	"version": "1.0.0",
	"alias": {
		"utils": "./src/js/utils"
	},
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/src.config.babel.js",
		"prod": "webpack --mode production --config ./.webpack/src.config.babel.js",
		"copy": "webpack --config ./.webpack/release-copy.config.js",
		"replace": "webpack --config ./.webpack/release-replace.config.js",
		"remove": "rm _extras/release/_replace.js _extras/release/_copy.js _extras/release/build/melonpan-block-images-front.js",
		"release": "npm run prod && npm run copy && npm run replace && npm run remove",
		"type": "tsc"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.4.4",
		"@types/lodash": "^4.14.133",
		"@types/node": "^12.0.4",
		"@types/react-dom": "^16.8.4",
		"@types/tinycolor2": "^1.4.2",
		"@types/uuid": "^3.4.4",
		"@wordpress/blocks": "^6.3.0",
		"@wordpress/components": "^7.4.0",
		"@wordpress/editor": "^9.3.0",
		"@wordpress/element": "^2.4.0",
		"@wordpress/i18n": "^3.4.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"lodash": "^4.17.11",
		"mini-css-extract-plugin": "^0.7.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"string-replace-loader": "^2.1.1",
		"style-loader": "^0.23.1",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.5.1",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2"
	},
	"dependencies": {
		"tinycolor2": "^1.4.1"
	}
}
